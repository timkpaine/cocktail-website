{
	"name": "barrelcocktail.co",
	"version": "0.1.0",
	"private": true,
	"homepage": "https://barrelcocktail.co",
	"type": "module",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"deploy": "rm -rf ./tmp-repo && git clone -b gh-pages git@github.com:streamlet-dev/website.git ./tmp-repo && rm -rf ./tmp-repo/* && cp -r build/. ./tmp-repo && cd ./tmp-repo && git add . && git commit -m 'autoupdate docs' && git push",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync",
		"check": "svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-check --tsconfig ./jsconfig.json --watch",
		"test": "playwright test",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. .",
		"fix": "yarn format"
	},
	"devDependencies": {
		"@playwright/test": "^1.22.2",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-static": "^1.0.0-next.34",
		"@sveltejs/kit": "next",
		"@tailwindcss/aspect-ratio": "^0.4.0",
		"@tailwindcss/forms": "^0.5.2",
		"@tailwindcss/line-clamp": "^0.4.0",
		"@tailwindcss/typography": "^0.5.2",
		"autoprefixer": "^10.4.7",
		"cssnano": "^5.1.12",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.14",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.1.4"
	},
	"dependencies": {
		"svelte-i18n": "^3.4.0",
		"svelte-navigator": "^3.1.6",
		"svelte-preprocess": "^4.10.7",
		"svelte-routing": "^1.6.0",
		"svelte-stripe-js": "^0.0.12"
	}
}
